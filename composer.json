{
  "name": "marievych/roles",
  "description": "Powerful package for handling roles and permissions in Laravel 5.5",
  "keywords": [
    "roles",
    "permissions",
    "acl",
    "auth",
    "laravel",
    "illuminate"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Yevhen Marievych",
        "email": "yevhen.marievych@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "laravel/framework": "5.5.*"
  },
  "autoload": {
    "psr-4": {
      "Marievych\\Roles\\": "src/"
    }
  },
  "require-dev": {
    "orchestra/database": "3.5.*",
    "orchestra/testbench": "3.5.*",
    "mockery/mockery": "^0.9.6",
    "phpunit/phpunit": "~6.0",
    "satooshi/php-coveralls": "1.0.*",
    "sebastian/environment": "2.0.*"
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php",
      "tests/User.php"
    ]
  }
}
